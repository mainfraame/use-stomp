{
  "author": "dayne mentier",
  "name": "nyse-web-tools-common",
  "version": "6.1.9-rc.4",
  "description": "common components and utilities",
  "readme": "README.md",
  "license": "MIT",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "cjs/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "NODE_ENV=production ./build.sh -build -styleguidist",
    "build:dev": "NODE_ENV=development ./build.sh -build -pack",
    "build:watch": "NODE_ENV=development ./buildDev.sh -build",
    "format": "prettier --check \"{,!(node_modules|cjs|esm|gradle)/**/}*.{ts,tsx}\" --loglevel warn -w",
    "lint": "eslint src/**/*.{ts,tsx} --ext .ts --ext .tsx --ignore-path ./.eslintignore --fix --quiet",
    "lint:check": "eslint src/**/*.{ts,tsx} --ext .ts --ext .tsx --ignore-path ./.eslintignore",
    "start": "npm i && npm run build:dev",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "test": "JEST_DEFAULT_TIMEOUT=30000 JEST_RETRY_TIMES=3 node --expose-gc $(npm bin)/jest --silent --logHeapUsage --verbose --coverage --testRunner=jest-circus/runner --maxWorkers=50%",
    "test:dev": "JEST_DEFAULT_TIMEOUT=20000 JEST_RETRY_TIMES=2 node --expose-gc $(npm bin)/jest --silent --logHeapUsage --verbose --coverage --testRunner=jest-circus/runner --maxWorkers=50%"
  },
  "repository": {
    "type": "git",
    "url": "https://stash.intcx.net/scm/aw/nyse-web-tools-common.git"
  },
  "keywords": [
    "components",
    "react"
  ],
  "publishConfig": {
    "registry": "https://artifactory.intcx.net/artifactory/api/npm/ice-npm"
  },
  "dependencies": {
    "@ag-grid-community/react": "~24.0.0",
    "@ag-grid-enterprise/all-modules": "~24.0.0",
    "@js-joda/core": "~3.1.0",
    "@js-joda/timezone": "~2.3.0",
    "@material-ui/core": "~4.11.0",
    "@material-ui/icons": "~4.9.1",
    "@material-ui/lab": "~4.0.0-alpha.56",
    "@material-ui/styles": "~4.10.0",
    "axios": "~0.20.0",
    "chroma-js": "2.1.0",
    "clean-deep": "3.4.0",
    "cleave.js": "~1.6.0",
    "clsx": "~1.1.1",
    "copy-text-to-clipboard": "~2.2.0",
    "core-js": "~3.6.5",
    "decimal.js": "~10.2.0",
    "deepmerge": "~4.2.2",
    "downloadjs": "~1.4.7",
    "formik": "~2.1.5",
    "lodash": "~4.17.20",
    "natural-compare-lite": "~1.4.0",
    "path-to-regexp": "^6.2.0",
    "react": ">=16.9.0 <=16.13.1",
    "react-day-picker": "~7.2.3",
    "react-dom": ">=16.9.0 <=16.13.1",
    "react-dom-factories": "~1.0.2",
    "react-fast-compare": "~3.2.0",
    "react-measure": "~2.3.0",
    "react-redux": "~7.2.0",
    "react-use": "~15.3.3",
    "react-virtualized": "~9.21.2",
    "recoil": "~0.0.10",
    "redux": "~4.0.5",
    "safe-stable-stringify": "~1.1.1",
    "styled-components": "~4.2.1",
    "xlsx": "~0.16.5",
    "yup": "~0.27.0"
  },
  "devDependencies": {
    "@aw-gherkin/core": "1.2.14",
    "@aw-gherkin/mocks": "0.1.0-alpha.5",
    "@babel/cli": "~7.12.1",
    "@babel/core": "~7.12.3",
    "@babel/plugin-proposal-class-properties": "~7.12.1",
    "@babel/plugin-proposal-decorators": "~7.12.1",
    "@babel/plugin-transform-react-constant-elements": "^7.12.1",
    "@babel/plugin-transform-runtime": "~7.12.1",
    "@babel/plugin-transform-typescript": "~7.12.1",
    "@babel/preset-env": "~7.12.1",
    "@babel/preset-react": "~7.12.1",
    "@babel/preset-typescript": "~7.12.1",
    "@testing-library/jest-dom": "~5.1.1",
    "@testing-library/react": "10.0.4",
    "@testing-library/react-hooks": "3.2.1",
    "@types/chance": "~1.0.7",
    "@types/chroma-js": "~2.1.0",
    "@types/classnames": "~2.2.9",
    "@types/cleave.js": "~1.4.3",
    "@types/cucumber": "~6.0.1",
    "@types/downloadjs": "~1.4.2",
    "@types/jest": "^26.0.15",
    "@types/lodash": "~4.14.162",
    "@types/natural-compare-lite": "~1.4.0",
    "@types/node": "~12.19.5",
    "@types/qs": "6.9.4",
    "@types/react": "~16.9.44",
    "@types/react-dom": "~16.9.8",
    "@types/react-redux": "~7.1.9",
    "@types/react-virtualized": "9.21.10",
    "@types/styled-components": "~5.1.2",
    "@types/yup": "~0.26.24",
    "@typescript-eslint/eslint-plugin": "~4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-plugin-lodash": "~3.3.4",
    "babel-plugin-macros": "2.8.0",
    "babel-plugin-set-display-name": "1.1.2",
    "babel-plugin-styled-components": "~1.11.1",
    "chance": "~1.1.3",
    "cucumber-jest": "^0.0.8",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "~23.0.2",
    "eslint-plugin-react": "~7.16.0",
    "eslint-plugin-react-hooks": "~2.2.0",
    "hooks.macro": "1.1.2",
    "husky": "~4.2.5",
    "import-sort-style-module": "6.0.0",
    "jest": "26.6.0",
    "jest-canvas-mock": "2.2.0",
    "jest-circus": "26.6.0",
    "jest-date-mock": "^1.0.8",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "prettier": "^2.1.2",
    "prettier-plugin-import-sort": "0.0.6",
    "react-styleguidist": "^9.0.4",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "~1.2.2",
    "thread-loader": "2.1.3",
    "typescript": "~4.0.3",
    "webpack": "~4.43.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format && npm run build:dev && git add ."
    }
  },
  "jest": {
    "projects": [
      {
        "displayName": "INTEGRATION",
        "collectCoverageFrom": [
          "<rootDir>/src/**/**.{js,jsx,ts,tsx}"
        ],
        "coveragePathIgnorePatterns": [
          "AgGridCtx",
          "AgModalCtx",
          "AgTooltipCtx",
          "index",
          "node_modules",
          "NyFormCtx",
          "NyFormDebug",
          "NyFooter",
          "Scenario",
          "spec",
          "jest.utils.tsx",
          "cucumber.scenarios.ts"
        ],
        "forceExit": true,
        "moduleDirectories": [
          "node_modules"
        ],
        "moduleFileExtensions": [
          "js",
          "jsx",
          "json",
          "ts",
          "tsx",
          "feature"
        ],
        "moduleNameMapper": {
          "^lodash-es$": "lodash"
        },
        "restoreMocks": true,
        "setupFiles": [
          "<rootDir>/node_modules/jest-canvas-mock",
          "<rootDir>/node_modules/@aw-gherkin/core/lib/drivers/jsdom/mocks/window.js"
        ],
        "setupFilesAfterEnv": [
          "<rootDir>/node_modules/cucumber-jest/dist/init.js",
          "<rootDir>/cucumber.setup.tsx"
        ],
        "testEnvironment": "jest-environment-jsdom-sixteen",
        "testMatch": [
          "<rootDir>/src/**/*.feature"
        ],
        "transform": {
          "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
          "^.+\\.(feature)$": "cucumber-jest"
        },
        "transformIgnorePatterns": [
          "node_modules/(?!lodash-es)"
        ],
        "testURL": "http://127.0.0.1/",
        "verbose": true
      },
      {
        "displayName": "UNIT",
        "collectCoverageFrom": [
          "<rootDir>/src/**/**.{js,jsx,ts,tsx}"
        ],
        "coveragePathIgnorePatterns": [
          "AgGridCtx",
          "AgModalCtx",
          "AgTooltipCtx",
          "index",
          "node_modules",
          "NyFormCtx",
          "NyFormDebug",
          "NyFooter",
          "Scenario",
          "spec",
          "jest.utils.tsx",
          "cucumber.scenarios"
        ],
        "moduleDirectories": [
          "node_modules"
        ],
        "moduleNameMapper": {
          "^lodash-es$": "lodash"
        },
        "setupFiles": [
          "<rootDir>/node_modules/@aw-gherkin/core/lib/drivers/jsdom/mocks/window.js",
          "<rootDir>/jest.setup.ts",
          "<rootDir>/node_modules/jest-canvas-mock",
          "<rootDir>/node_modules/jest-date-mock"
        ],
        "setupFilesAfterEnv": [
          "<rootDir>/jest.setupAfterEnv.ts"
        ],
        "testEnvironment": "jest-environment-jsdom-sixteen",
        "testMatch": [
          "<rootDir>/src/**/**.spec.*"
        ],
        "transform": {
          "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
        },
        "transformIgnorePatterns": [
          "node_modules/(?!lodash-es)"
        ],
        "testURL": "http://127.0.0.1/",
        "verbose": true
      }
    ]
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "embeddedLanguageFormatting": "auto",
    "htmlWhitespaceSensitivity": "strict",
    "insertPragma": false,
    "jsxBracketSameLine": true,
    "jsxSingleQuote": true,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": false
  },
  "sideEffects": false
}
